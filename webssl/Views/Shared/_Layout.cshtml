<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/Content/chosen")
    @Styles.Render("~/Content/jsTree3")


</head>
<body>
    <div id="UserInfo" class="UserInfo"></div>

    @Html.Partial("HeaderPartial")


    <div class="container body-content" style="padding-top:10px;">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - a master thesis project</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/ticket")
    @Scripts.Render("~/bundles/bootstrap")

    @Scripts.Render("~/bundles/chosen")
    @Scripts.Render("~/bundles/jsTree3")
    @Scripts.Render("~/bundles/Custom")
    @Scripts.Render("~/bundles/jqueryval")
    @RenderSection("scripts", required: false)

    <div id="modal-container" class="modal fade" tabindex="-1" role="dialog">

        <div class="modal-dialog">
            <div class="modal-content">
            </div>
        </div>

    </div>


</body>
</html>
