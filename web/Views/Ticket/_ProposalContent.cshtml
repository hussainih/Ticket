@model Ticketing.TicketModel.Proposal
<!-- Modal -->
<div class="panel panel-default">
    <div class="panel-heading">
        <h4 class="panel-title">
            <a data-toggle="collapse" href="#collapse2">@Model.Name</a>
            <a style="float:right;" class="glyphicon glyphicon-edit" data-toggle="modal" data-target="#ProposalContentID"></a>
        </h4>
    </div>
    <div id="collapse2" class="panel-collapse collapse">
        <div class="panel-body">

            @Html.Raw(@Model.ProposalContent.Content)
        </div>
        <div class="panel-footer">Comment Area</div>
    </div>
</div>
